{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "react",
    [
      "babili",
      {
        "evaluate": false,
        "mangle": false
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties"
  ],
}
